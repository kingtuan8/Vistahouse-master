@model IEnumerable<NhomXingfa.Areas.Quantri.Models.DataModels.PhotoLibraryCategory>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Quantri/Views/Shared/_Layout.cshtml";
}
<div id="page-heading">
    <ol class="breadcrumb">
        <li><a href="/">Trang chủ</a></li>
        <li class="active">Danh mục Thư viện Ảnh</li>

    </ol>

    <h1>Danh mục Thư viện Ảnh</h1>
    <div class="options">
        <div class="btn-toolbar">

            <a href="/Quantri/PhotoLibraryCategories/Create/" class="btn btn-success btn-label"><i class="fa fa-plus"></i>Thêm mới</a>
        </div>
    </div>
</div>
<div class="container" style="background-color:#ffffff">
    <table class="table">
        <tr>
            <th>
                Tên Danh Mục
            </th>
            <th>
                Mô tả
            </th>
            <th>
                Ngày tạo
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LibraryName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LibraryDesc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Created)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
    </div>