@model NhomXingfa.Areas.Quantri.Models.DataModels.CustomerFeedback

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Quantri/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div class="container" style="background-color:#ffffff">
    <h4>CustomerFeedback</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Khách Hàng
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerName)
        </dd>

       

        <dt>
           Thông tin
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerInfo)
        </dd>

        <dt>
           Cảm nhận
        </dt>

        <dd>
            <p style="font-weight:bold; font-style:italic">
                @Html.DisplayFor(model => model.Feedback)
            </p>
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ThuTu)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ThuTu)
        </dd>

        
        <dt>
           Tạo lúc
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Created)
        </dd>

        <dt>
            Cập nhật cuối
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastModify)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a href="/Quantri/CustomerFeedbacks/Index">Quay Lại</a>
        </div>
    }
</div>
